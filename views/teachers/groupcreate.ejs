<!doctype html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html" charset="utf-8" />
	<title>Admin - New Group</title>
	<link rel="stylesheet" href="../../../css/bootstrap.min.css" title="Bootstrap" type="text/css" media="screen" charset="utf-8">
	<link rel="stylesheet" href="../../../css/bootstrap-responsive.min.css" title="Bootstrap responsive" type="text/css" media="screen" charset="utf-8">

	
</head>
<body>
	
	<h1>New Group</h1>
	
	<% if ( message ) { %>
	<%= message %>
	<% } %>
	
	<form method="post" action="/teachers/groups/create">
		<div>
	        <label>Teacher:</label>
	        <input type="text" name="teacher"/>
	    </div>
		<div>
	        <label>Year:</label>
	        <input type="number" name="year"/>
	    </div>
	    <div>
	        <label>Name:</label>
	        <input type="text" name="habitsGroup"/>
	    </div>
	    <div>
	        <input type="submit" value="Create"/>
	    </div>	
	</form>
	
	

</body>
</html>